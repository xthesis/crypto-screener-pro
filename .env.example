# Environment Variables Template
# Copy this to .env.local and fill in your values

# Supabase (Database)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Telegram Bot
TELEGRAM_BOT_TOKEN=123456789:ABC-DEF_your_bot_token_here

# Stripe (Payments)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here
STRIPE_SECRET_KEY=sk_test_your_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
NEXT_PUBLIC_STRIPE_PRO_PRICE_ID=price_your_pro_plan_id
NEXT_PUBLIC_STRIPE_TEAM_PRICE_ID=price_your_team_plan_id

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# API Keys (Optional - for higher limits)
COINGECKO_API_KEY=your_coingecko_pro_key_optional
